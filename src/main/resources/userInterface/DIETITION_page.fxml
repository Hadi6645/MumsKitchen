<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="userInterface.DIETITION_page">
   <children>
      <Label layoutX="180.0" layoutY="59.0" text="update menu" textFill="#c40000">
         <font>
            <Font name="Traditional Arabic" size="27.0" />
         </font>
      </Label>
      <Label layoutX="64.0" layoutY="174.0" text="update by meal" />
      <Label layoutX="64.0" layoutY="232.0" text="update by drink" />
      <Label layoutX="58.0" layoutY="294.0" text="update by dessert" />
      <Button fx:id="add_meal_B" layoutX="75.0" layoutY="372.0" mnemonicParsing="false" onAction="#add_meal_act" prefHeight="25.0" prefWidth="78.0" text="add meal" />
      <Button fx:id="add_drink_B" layoutX="198.0" layoutY="372.0" mnemonicParsing="false" onAction="#add_drink_act" prefHeight="25.0" prefWidth="78.0" text="add drink" />
      <Button fx:id="add_dessert_B" layoutX="325.0" layoutY="372.0" mnemonicParsing="false" onAction="#add_dessert_act" text="add dessert" />
      <Button fx:id="commit_B" layoutX="159.0" layoutY="443.0" mnemonicParsing="false" onAction="#commit_updates_act" prefHeight="25.0" prefWidth="155.0" text="commit update" />
      <ChoiceBox fx:id="choiceBox_meal" layoutX="175.0" layoutY="170.0" onDragDetected="#updateItem" prefWidth="150.0" />
      <ChoiceBox fx:id="choiceBox_drink" layoutX="175.0" layoutY="228.0" onDragDetected="#updateItem" prefWidth="150.0" />
      <ChoiceBox fx:id="choiceBox_dessert" layoutX="175.0" layoutY="290.0" onDragDetected="#updateItem" prefWidth="150.0" />
   </children>
</AnchorPane>
