<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.ColorAdjust?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="userInterface.Telaviv_main">
   <children>
      <ImageView fitHeight="500.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/telaviv.png" />
         </image>
         <effect>
            <ColorAdjust brightness="0.11" contrast="-0.22" />
         </effect>
      </ImageView>
      <Button fx:id="shipping_order_B" layoutX="28.0" layoutY="58.0" mnemonicParsing="false" onAction="#order_act" text="shipping order">
         <font>
            <Font name="Traditional Arabic" size="17.0" />
         </font>
      </Button>
      <Button fx:id="resevation_B" layoutX="28.0" layoutY="166.0" mnemonicParsing="false" onAction="#resevation_act" prefHeight="38.0" prefWidth="121.0" text="reservation" textAlignment="CENTER">
         <font>
            <Font name="Traditional Arabic" size="17.0" />
         </font>
      </Button>
      <Button fx:id="contact_us_B" layoutX="28.0" layoutY="261.0" mnemonicParsing="false" onAction="#contact_act" prefHeight="38.0" prefWidth="121.0" text="contact us" textAlignment="CENTER">
         <font>
            <Font name="Traditional Arabic" size="17.0" />
         </font>
      </Button>
      <Button fx:id="log_in_B" layoutX="28.0" layoutY="346.0" mnemonicParsing="false" onAction="#log_act" prefHeight="41.0" prefWidth="121.0" text="log in" textAlignment="CENTER">
         <font>
            <Font name="Traditional Arabic" size="17.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
